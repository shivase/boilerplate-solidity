{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "format": "yarn prettier --write contracts/**/*.sol",
    "lint": "yarn format && yarn solhint -f table contracts/**/*.sol",
    "accounts": "yarn hardhat accounts",
    "compile": "yarn hardhat compile",
    "clean": "yarn hardhat clean",
    "test": "TS_NODE_TRANSPILE_ONLY=1 yarn hardhat test",
    "test:gas": "TS_NODE_TRANSPILE_ONLY=1 REPORT_GAS=true yarn hardhat test",
    "coverage": "yarn hardhat coverage",
    "node": "yarn hardhat node",
    "deploy:localhost": "yarn hardhat deploy --network localhost",
    "deploy:ganache": "yarn hardhat deploy --network ganache"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.0",
    "@truffle/hdwallet-provider": "^2.0.9",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.9.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethers": "^5.0.0",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.9.9",
    "hardhat-deploy": "^0.11.11",
    "hardhat-deploy-tenderly": "^0.2.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solc": "^0.8.15",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
